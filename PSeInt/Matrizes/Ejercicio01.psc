Algoritmo Ejercicio01
	Dimension A[15]
	Dimension B[15]
	

	Para i<-0 Hasta 14 Con Paso 1 Hacer
		Escribir "Ingrese el numero vector A ",i
		Leer A[i]
		Escribir "Ingrese el numero vector B ",i
		Leer B[i]
	Fin Para
	
	Para i<-0 Hasta 14 Con Paso 1 Hacer
		suma=A[i]+B[i]
		Escribir "Esta es la suma de los vectores ",suma "# ",i
	Fin Para

FinAlgoritmo
